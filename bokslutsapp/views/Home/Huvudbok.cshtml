@model IEnumerable<Bokslutsapp.Models._1930Bank>

@{
    ViewBag.Bilaga = "1";
    ViewBag.Konto = "1930";
    ViewBag.Title = "Huvudbok";
    ViewBag.Company = "Företag AB";
    ViewBag.OrgNr = "556800-5000";
    ViewBag.FiscalYear = "2017-01-01 - 2017-12-31";
    ViewBag.VerNr = "1";

    ViewBag.testsumma = "786503,79";
    ViewBag.testdiff = "0";
    ViewBag.avskiljare = ", ";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>

<!--BILAGA-->
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <!--KONTON-->
        <tr>
            <th width="10%">Konto</th>
            <th width="3%">KS</th>
            <th width="3%">Pr</th>
            <th width="45%">Benämning</th>
            <th width="10%">Verifikation</th>
            <th width="15%">Datum</th>
            <th width="14%">Belopp</th>
        </tr>
        <tr>
            <td align="left" contenteditable='true'>50*</td>
            <td align="center" contenteditable='true'>123</td>
            <td align="center" contenteditable='true'>456</td>
            <td align="left" contenteditable='true'>*bank</td>
            <td align="center" contenteditable='true'>A25*</td>
            <td align="center" contenteditable='true'>20170101</td>
            <td align="right" contenteditable='true'>50000</td>
        </tr>
        @foreach (var Bilaga in Model) 
        {
            <tr>
                <td align="left" contenteditable='true'>@Bilaga.Konto</td>
                <td align="center" contenteditable="true">@Bilaga.Ks</td>
                <td align="center" contenteditable="true">@Bilaga.Pr</td>
                <td align="left" contenteditable='true'>@Bilaga.Beskrivning</td>
                <td align="center" contenteditable='true'>@Bilaga.Serie@Bilaga.VerifikationNr</td>
                <td align="center" contenteditable='true'>@Bilaga.Datum.ToString("yyyy-MM-dd")</td>
                <td align="right" contenteditable='true' >@Bilaga.Belopp</td>                
            </tr>
        }
        <tr>
            <th colspan="6"><strong>Summa</strong></th>
            <td id="sum" align="right"><strong></strong></td>
        </tr>
    </thead>
</table>


<!--Konteringsvy-->
<div class="content-wrapper">
    @Html.Partial("_footer")
</div>
</html>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.0/jquery.min.js">
</script>
<script>
    $(document).ready(function () {
        
    });
</script>

