<script>
    $(document).ready(function () {
        $('#skattespec').keyup(function () {
            var resfskatt = $('#resfsk').html();
            var ejavdrg = $('#ejavdr').html();
            var ejskattepl = $('#ejskpl').html();
            var skattemjust = $('#skjust').html();
            var schablpfond = $('#pfond').html();
            var underskavdr = $('#undavd').html();
            var berskattemres = resfskatt + ejavdrg - ejskattepl + skattemjust + schablpfond - underskavdr;
            var skatt = berskattemres * $('#skattesats').html();

            $("#resfsk").html(resfskatt);
            $("#ejavdr").html(ejavdrg);
            $("#ejskpl").html(ejskattepl);

            $("#berskattres").html(berskattemres);
            $("#skfres").html(skatt);
        }
    )});
</script>