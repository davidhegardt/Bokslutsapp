@model IEnumerable<Bokslutsapp.Models._1930Bank>

@{
    ViewBag.Bilaga = "1";
    ViewBag.Konto = "1930";
    ViewBag.Title = "Kassa";
    ViewBag.Company = "Företag AB";
    ViewBag.OrgNr = "556800-5000";
    ViewBag.FiscalYear = "2017-01-01 - 2017-12-31";
    ViewBag.VerNr = "1";

    ViewBag.testsumma = "786503,79";
    ViewBag.testdiff = "0";
    ViewBag.avskiljare = ", ";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>

<!--Sidhuvud-->
<body>
    <div class="content-wrapper">
        @Html.Partial("_header")
    </div>
</body>
</html>

<!--BILAGA-->

<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <!--KONTON-->
        <tr>
            <th width="85%">Kontosaldo enligt kassainventering</th>
            <th width="15%">Belopp</th>
        </tr>
       
        <tr>
            <td align="left" contenteditable='true'></td>
            <td align="right" contenteditable='true'></td>
        </tr>
        
        <tr>
            <th><strong>Summa</strong></th>
            <td align="right"><strong>@String.Format("{0:#,###.##}", ViewBag.testsumma)</strong></td>
        </tr>
    </thead>
</table>

<table id="example" class="table table-striped table-bordered" style="width:100%">
    <!--Avstämning bokförda uttag-->
    <thead>
        <tr>
            <th colspan="1" width="10%">Datum</th>
            <th colspan="1" width="8%">Referens</th>
            <th colspan="8" width="61%">Ej bokförda utbetalningar (kvitton i kassan)</th>
            <th colspan="1" width="6%">Konto</th>
            <th colspan="1" width="15%">Belopp</th>
        </tr>
        @for (var i = 0; i < 3; i++)
        {
            <tr>
                <td colspan="1" align="left"   contenteditable='true'></td>
                <td colspan="1" align="center" contenteditable='true'></td>
                <td colspan="8" align="left"   contenteditable='true'></td>
                <td colspan="1" align="center" contenteditable='true'></td>
                <td colspan="1" align="right"  contenteditable='true'></td>
            </tr>
        }
        <tr>
            <th colspan="11"><strong>Summa</strong></th>
            <td colspan="1" align="right"><strong>@String.Format("{0:#,###.##}", ViewBag.testsumma)</strong></tdcolspan="1">
        </tr>
</table>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <!--Avstämning bokförda insättningar-->
    <thead>
        <tr>
            <th colspan="1" width="10%">Datum</th>
            <th colspan="1" width="8%">Referens</th>
            <th colspan="8" width="61%">Checkar, kuponger, presentkort och liknande som inte bokförts</th>
            <th colspan="1" width="6%">Konto</th>
            <th colspan="1" width="15%">Belopp</th>
        </tr>
        @for (var i = 0; i < 3; i++)
        {
        <tr>
            <td colspan="1" align="left"    contenteditable='true'></td>
            <td colspan="1" align="center"  contenteditable='true'></td>
            <td colspan="8" align="left"    contenteditable='true'></td>
            <td colspan="1" align="center"  contenteditable='true'></td>
            <td colspan="1" align="right"   contenteditable='true'></td>
        </tr>
        }
        <tr>
            <th colspan="11" width="85%"><strong>Summa</strong></th>
            <td colspan="1" width="15%" align="right"><strong>@String.Format("{0:#,###.##}", ViewBag.testsumma)</strong></tdcolspan="1">
        </tr>
</table>
<table id="example" class="table table-striped table-bordered" style="width:100%">
    <!--Avstämning ej bokförda uttag-->
    <thead>
        <tr>
            <th colspan="1" width="10%">Datum</th>
            <th colspan="1" width="8%">Referens</th>
            <th colspan="8" width="61%">Ej bokförd inbetalningar till kassan (avgår vid kontroll mot bokföringen)</th>
            <th colspan="1" width="6%">Konto</th>
            <th colspan="1" width="15%">Belopp</th>
        </tr>
        @for (var i = 0; i < 3; i++)
        {
        <tr>
            <td colspan="1" align="left"    contenteditable='true'></td>
            <td colspan="1" align="center"  contenteditable='true'></td>
            <td colspan="8" align="left"    contenteditable='true'></td>
            <td colspan="1" align="center"  contenteditable='true'></td>
            <td colspan="1" align="right"   contenteditable='true'></td>
        </tr>
        }
        <tr>
            <th colspan="11"><strong>Summa</strong></th>
            <td colspan="1" align="right"><strong>@String.Format("{0:#,###.##}", ViewBag.testsumma)</strong></tdcolspan="1">
        </tr>
</table>

<table id="example" class="table table-striped table-bordered" style="width:100%">
    <!--Summarad-->
    <thead>
        <tr>
            <th width="40%">Kassabehållningen intygad av/den:</th>
            <th width="45%"><strong>Totalsumma enligt bilagan</strong></th>
            <td width="15%" align="right"><strong>@String.Format("{0:#,###.##}", ViewBag.testsumma)</strong></td>
        </tr>
        <tr>
            <td contenteditable="true"></td>
            <th><strong>Saldo enligt huvudbok</strong></th>
            <td align="right"><strong>@ViewBag.testdiff</strong></td>
        </tr>
        <tr>
            <td contenteditable="true"></td>
            <th><strong>Belopp kvar att specificera</strong></th>
            <td align="right"><strong>@ViewBag.testdiff</strong></td>
        </tr>
    </thead>
</table>

<p align="right">
    <a href="#" class="btn btn-success btn-lg">
        <span class="glyphicon glyphicon-trash" data-toggle="tooltip" title="Töm bilaga"></span>
    </a>
    <a href="#" class="btn btn-success btn-lg">
        <span class="glyphicon glyphicon-ok" data-toggle="tooltip" title="Klarmarkera bilaga"></span>
    </a>
</p>


<!--Konteringsvy-->
<div class="content-wrapper">
    @Html.Partial("_footer")
</div>

